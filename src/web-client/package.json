{
  "name": "confidential-cat-counter-web",
  "version": "1.0.0",
  "description": "Web client for the Confidential Cat Counter privacy-preserving ML reference architecture",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:audit": "npm audit --audit-level=moderate",
    "security:licenses": "license-checker --summary --onlyAllow 'MIT;ISC;BSD-2-Clause;BSD-3-Clause;Apache-2.0;Unlicense'",
    "docker:build": "docker build -t confidential-cat-counter-web .",
    "docker:run": "docker run -p 3000:3000 -e REDIS_URL=redis://localhost:6379 confidential-cat-counter-web"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!tests/**",
      "!coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testTimeout": 30000
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "license-checker": "^25.0.1",
    "nodemon": "^3.0.0",
    "prettier": "^3.2.0",
    "supertest": "^6.3.0"
  },
  "keywords": [
    "privacy",
    "machine-learning",
    "encryption",
    "confidential-computing",
    "reference-architecture"
  ],
  "author": "Confidential Cat Counter Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jellywish/confidential-cat-counter.git"
  },
  "bugs": {
    "url": "https://github.com/jellywish/confidential-cat-counter/issues"
  },
  "homepage": "https://github.com/jellywish/confidential-cat-counter#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}